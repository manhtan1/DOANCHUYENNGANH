}
<style>
    .color_white {
        color: #fff !important;
    }

    .sits__row span {
        color: #000;
    }

    .sits .sits__indecator {
        display: inline-block;
        width: 30px;
        height: 30px;
        margin: 5px;
        border: 1px solid #4c4145;
        font-size: 13px;
        padding-top: 5px;
        background: #fff;
        color: #000;
    }

    .sits .sits__row .sits__place {
        position: relative;
        margin: 5px;
        width: 30px;
        height: 30px;
        display: inline-block;
        line-height: 30px;
        font-size: 10px;
        color: #5a5959;
        z-index: 13;
        /* text-indent: -9999px; */
    }

        .sits .sits__row .sits__place:hover {
            text-indent: 0;
            color: #fff;
            cursor: pointer;
        }

    .tickets_film {
        width: 300px;
        height: 255px;
        border: 1px solid #000;
        box-shadow: 8px 10px 20px #1f2020;
        border-radius: 3px;
        position: absolute;
        right: -130px;
        top: 52%;
    }

    .bg-gray-400 {
        --tw-bg-opacity: 1;
        background-color: rgb(84, 130, 209);
    }

    hr {
        margin: 0;
        border: none;
    }

    .price_film {
        height: 20%;
        text-align: center;
        display: flex;
        align-items: center;
        justify-content: center;
    }

    .name_film {
        height: 20%;
        padding: 2px 15px;
    }

    .date_watch {
        height: 20%;
        padding: 2px 15px;
    }

    .chair {
        height: 20%;
        padding: 2px 15px;
    }

    .buy_tickets {
        width: 100%;
        height: 20%;
        padding: 15px 0 14px 0;
        margin-bottom: -1px;
        background: #ef4444;
        color: #fff;
        border: none;
        border-radius: 3px;
    }

    .choose-sits {
        position: relative;
    }


    .text_title {
        color: #16a34a;
        font-weight: bold;
    }
</style>
